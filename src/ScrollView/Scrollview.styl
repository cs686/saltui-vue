.{$prefix}-scroll-view {
  position relative
  height 100%
  overflow-x hidden
  overflow-y auto
  -webkit-overflow-scrolling touch
}

.vc-scrolldemo {
  color: black;
  padding: 30px 10px;
  text-align: center;
  background: #a9c8f4;
}

